# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=flourish-relationship-app.firebaseapp.com
FIREBASE_PROJECT_ID=flourish-relationship-app
FIREBASE_STORAGE_BUCKET=flourish-relationship-app.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/flourish_db
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# API Keys
OPENAI_API_KEY=your_openai_api_key
HUGGING_FACE_API_KEY=your_hugging_face_api_key
COHERE_API_KEY=your_cohere_api_key

# Payment Processing
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@flourish-app.com

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=flourish-app-storage

# Social Media API Keys
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
APPLE_CLIENT_ID=your_apple_client_id
APPLE_TEAM_ID=your_apple_team_id
APPLE_KEY_ID=your_apple_key_id
APPLE_PRIVATE_KEY=your_apple_private_key

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_PRIVATE_KEY=your_apns_private_key

# Analytics
GOOGLE_ANALYTICS_ID=your_google_analytics_id
MIXPANEL_TOKEN=your_mixpanel_token
SEGMENT_WRITE_KEY=your_segment_write_key

# Monitoring
SENTRY_DSN=your_sentry_dsn
DATADOG_API_KEY=your_datadog_api_key

# App Store Configuration
APPLE_ID=your_apple_id
APPLE_ID_PASSWORD=your_apple_id_password
APPLE_APP_SPECIFIC_PASSWORD=your_app_specific_password
APPLE_TEAM_ID=your_apple_team_id
APPLE_ITC_TEAM_ID=your_itc_team_id

# Android Configuration
ANDROID_KEYSTORE_FILE=android-release-key.keystore
ANDROID_KEY_ALIAS=release-key
ANDROID_KEYSTORE_PASSWORD=your_keystore_password
ANDROID_KEY_PASSWORD=your_key_password

# Development
NODE_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Production
DOMAIN=flourish-app.com
API_URL=https://api.flourish-app.com
WEB_URL=https://flourish-app.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# File Upload
MAX_FILE_SIZE=50MB
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/quicktime

# Encryption
ENCRYPTION_KEY=your_encryption_key
HASH_SALT_ROUNDS=12

# Feature Flags
ENABLE_AI_COACHING=true
ENABLE_VIDEO_CALLS=true
ENABLE_PREMIUM_FEATURES=true
ENABLE_LOCATION_MATCHING=true

# Third-party Services
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
ZOOM_API_KEY=your_zoom_api_key
ZOOM_API_SECRET=your_zoom_api_secret